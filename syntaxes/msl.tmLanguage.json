{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "MSL",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.msl",
					"match": "\\b(BREAK|IF|ELSE|ENDIF|LOOP|ENDLOOP|WAIT)\\b"
				},
				{
					"name": "keyword.constant.msl",
					"match": "\\b(PARAM|ODBSET|ODBGET|ODBCREATE)\\b"
				}
			]
		},
		"comments": {
			"patterns": [
				{
				"captures": {
					"0": {
						"name": "punctuation.definition.comment.msl"
					}
				},
				"match": "#.*",
				"name": "comment.line.number-sign.msl"
			}
			]
		},
		"strings": {
			"name": "string.quoted.double.msl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.msl",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.msl"
}
{
  "name": "midas-sequencer",
  "displayName": "midas-sequencer",
  "description": "MIDAS Scripting Language (MSL) support",
  "publisher": "maiahw",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "midas-sequencer",
        "aliases": [
          "MSL",
          "msl",
          "midas-sequencer"
        ],
        "extensions": [
          ".msl"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/midas_logo.png",
          "dark": "./images/midas_logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "midas-sequencer",
        "scopeName": "source.midas-sequencer",
        "path": "./syntaxes/msl.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maia-hw/midas-sequencer-support"
  }
}
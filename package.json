{
  "name": "midas-sequencer",
  "displayName": "midas-sequencer",
  "description": "MIDAS Scripting Language (MSL) support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "midas-sequencer",
      "aliases": ["MSL", "msl", "midas-sequencer"],
      "extensions": [".msl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "midas-sequencer",
      "scopeName": "source.midas-sequencer",
      "path": "./syntaxes/msl.tmLanguage.json"
    }]
  }
}
